    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "dist",
      "**/*.spec.(ts|tsx)"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ]
